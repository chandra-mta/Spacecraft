<script language="JavaScript">
    function WindowOpener(imgname) {
        msgWindow = open("","displayname","toolbar=no,directories=no,menubar=no,location=no,scrollbars=no,status=no,,width=1100,height=900,resize=no");
        msgWindow.document.clear();
        msgWindow.document.write("<html><title>Trend plot:   "+imgname+"</TITLE>");
        msgWindow.document.write("<body>");
        msgWindow.document.write("<img src='http://cxc.harvard.edu/mta_days/mta_otg/OTG_Plots/"+imgname+"' border =0 style='width:90%;'><p></p></body></hvtml>")
        msgWindow.document.close();
        msgWindow.focus();
    }
</script>

